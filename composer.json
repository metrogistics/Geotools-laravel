{
    "name": "metrogistics/geotools-laravel",
    "description": "Geo-related tools PHP library for Laravel 4 & 5",
    "keywords": ["laravel", "geotools", "geometry", "geocoder", "geocoding", "geoip", "bounds", "distance", "batch", "async"],
    "homepage": "http://geotools-php.org/",
    "license": "MIT",
    "authors": [
        {
            "name": "Antoine Corcy",
            "email": "contact@sbin.dk",
            "homepage": "http://sbin.dk",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "support@geotools-php.org",
        "issues": "https://github.com/toin0u/Geotools-laravel/issues",
        "source": "https://github.com/toin0u/Geotools-laravel"
    },
    "require": {
        "php": ">=5.4",
        "illuminate/support": "~5.4",
        "league/geotools": "~0.7"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "orchestra/testbench": "~3.4"
    },
    "autoload": {
        "psr-4": {
            "MetroGistics\\Geotools\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MetroGistics\\Tests\\Geotools\\": "tests/"
        }
    }
}
